<%
' A Function to check if some field entered by user is empty
Function ChkString(string)
If string = "" Then string = " "
ChkString = Replace(string, "'", "''")
End Function


Function SplitNameValue(string)
SplitNameValue=Split(string,"_")
End Function

' Receiving values from Form

ACP_ID= ChkString(Request.Form("ACP_ID"))
ACP_First_Name= ChkString(Request.Form("ACP_First_Name"))
ACP_Last_Name= ChkString(Request.Form("ACP_Last_Name"))

q1 = SplitNameValue(Request.Form("Question_1"))
Question_1= ChkString(q1(0))
Score_1= ChkString(q1(1))

q2 = SplitNameValue(Request.Form("Question_2"))
Question_2= ChkString(q2(0))
Score_2= ChkString(q2(1))

q3 = SplitNameValue(Request.Form("Question_3"))
Question_3= ChkString(q3(0))
Score_3= ChkString(q3(1))

q4 = SplitNameValue(Request.Form("Question_4"))
Question_4= ChkString(q4(0))
Score_4= ChkString(q4(1))

q5 = SplitNameValue(Request.Form("Question_5"))
Question_5= ChkString(q5(0))
Score_5= ChkString(q5(1))

q6 = SplitNameValue(Request.Form("Question_6"))
Question_6= ChkString(q6(0))
Score_6= ChkString(q6(1))

q7 = SplitNameValue(Request.Form("Question_7"))
Question_7= ChkString(q7(0))
Score_7= ChkString(q7(1))

q8 = SplitNameValue(Request.Form("Question_8"))
Question_8= ChkString(q8(0))
Score_8= ChkString(q8(1))

q9 = SplitNameValue(Request.Form("Question_9"))
Question_9= ChkString(q9(0))
Score_9= ChkString(q9(1))

q10 = SplitNameValue(Request.Form("Question_10"))
Question_10= ChkString(q10(0))
Score_10= ChkString(q10(1))

q11 = SplitNameValue(Request.Form("Question_11"))
Question_11= ChkString(q11(0))
Score_11= ChkString(q11(1))


Overall_Comments= ChkString(Request.Form("Overall_Comments"))
PF= ChkString(Request.Form("PF"))


totalscoreN = Score_1 + Score_2 + Score_3 + Score_4 + Score_5 + Score_6 + Score_7 + Score_8 + Score_9 + Score_10 + Score_11
if totalscoreN<>0 then
totalscore = (((CDbl(Score_1)) + (CDbl(Score_2)) + (CDbl(Score_3)) + (CDbl(Score_4)) + (CDbl(Score_5)) + (CDbl(Score_6)) + (CDbl(Score_7)) + (CDbl(Score_8)) + (CDbl(Score_9)) + (CDbl(Score_10)) + (CDbl(Score_11))) / 75 ) * 100
end if  


Final_Score= Round(totalscore,0)

CurrDate = date()
Currtime = time()
ipaddr = Request.ServerVariables("REMOTE_ADDR")


Response.Write("Total QA Score: " & Final_Score & "%")
Response.Write ("<br>")
Response.Write ("<br>")


data_source = "Provider=Microsoft.Jet.OLEDB.4.0; Data Source=" & _
Server.MapPath("\database\Walgreens.mdb")

sql_insert = "insert into WalgreensQA(ACP_ID,ACP_First_Name,ACP_Last_Name,Question_1,Score_1,Question_2,Score_2,Question_3,Score_3,Question_4,Score_4,Question_5,Score_5,Question_6,Score_6,Question_7,Score_7,Question_8,Score_8,Question_9,Score_9,Question_10,Score_10,Question_11,Score_11,Overall_Comments,PF,Final_Score,CurrDate,Currtime,ipaddr) values ('" & ACP_ID & "','" & ACP_First_Name & "','" & ACP_Last_Name & "','" & Question_1 & "','" & Score_1 & "','" & Question_2 & "','" & Score_2 & "','" & Question_3 & "','" & Score_3 & "','" & Question_4 & "','" & Score_4 & "','" & Question_5 & "','" & Score_5 & "','" & Question_6 & "','" & Score_6 & "','" & Question_7 & "','" & Score_7 & "','" & Question_8 & "','" & Score_8 & "','" & Question_9 & "','" & Score_9 & "','" & Question_10 & "','" & Score_10 & "','" & Question_11 & "','" & Score_11 & "','" & Overall_Comments & "','" & PF & "','" & Final_Score & "','" & CurrDate & "','" & Currtime & "','" & ipaddr & "')"

' Creating Connection Object and opening the database
Set con = Server.CreateObject("ADODB.Connection")
con.Open data_source
con.Execute sql_insert

Set rs = con.Execute("Select @@Identity as x") 

Response.Write ("Your Submission ID = ")
Response.Write "<a href=""WQA_Index-ReadOnly.asp?ID=" & rs.Fields("x") & _ 
                 """TARGET=_BLANK>" 
Response.Write (rs("x")) & "</a></b></li>" 
Response.Write ("<br>")
Response.Write ("<br>")
Response.Write ("Please click on the link above to send and review your form")


' Done. Close the connection
con.Close
Set con = Nothing

Response.Write ("<br>")
Response.Write ("<br>")

